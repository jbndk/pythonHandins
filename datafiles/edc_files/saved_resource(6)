if (location.href.match("www.edc.dk/kampagne/2014-uge-41") || location.href.match(/www.edc.dk\/saadan-saelger-du\/$/)) {
    adfButton = document.querySelector(".btn--inverted input") || {};
    adfButton.onclick = function() {
        setTimeout(function() {
            if (!document.querySelector(".form__input--error")) {
                inputClass = document.title === "EDC : Kampagner : 2014 uge 41" ? ".col-12 input[type=\"radio\"]" : ".col-4 input[type=\"radio\"]";
                adfOption = document.querySelectorAll(inputClass);
                for (var i = 0; i < adfOption.length; i++) {
                    if (adfOption[i].checked) {
                        index = i + 1;
                        TPName = "LEAD|" + document.title.replace(/\s:\s/g , ":") + "|Salgtjek" + index;
                        adf.track(141717, TPName);
                        break;
                    };
                };
            }
        }, 100);
    };
}
